{
  "name": "akkor-hotel",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "nx run-many --targets=build --all",
    "start": "docker-compose -f docker/docker-compose.yml --env-file docker/.docker-compose.env -p all up -d --force-recreate",
    "docker:database": "docker-compose -f docker/docker-compose.yml --env-file docker/.docker-compose.env -p database up -d --force-recreate",
    "docker:down": "docker-compose -f docker/docker-compose.yml --env-file docker/.docker-compose.env down",
    "docker:stop": "docker-compose -f docker/docker-compose.yml --env-file docker/.docker-compose.env stop",
    "start:dev:backend": "pnpm run docker:down && pnpm run docker:database && nx serve backend",
    "docker:rm:test": "pnpm run docker:test:down",
    "docker:test": "pnpm run docker:stop && pnpm run docker:rm:test && pnpm run build && pnpm run docker:build:test && docker-compose -f test/docker-compose.yml up -d --force-recreate",
    "docker:test:down": "docker-compose -f test/docker-compose.yml down",
    "docker:build:test": "docker-compose -f test/docker-compose.yml build",
    "docker:build": "docker-compose -f docker/docker-compose.yml --env-file docker/.docker-compose.env build",
    "test": "nx affected:test --verbose --run-in-band --skip-nx-cache",
    "start:front": "nx serve frontend --proxy-config=tools/proxy.conf.json"
  },
  "private": true,
